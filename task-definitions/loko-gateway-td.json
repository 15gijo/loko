{
  "family": "loko-gateway-td",
  "containerDefinitions": [
    {
      "name": "loko-gateway",
      "image": "REPLACEME_IMAGE",
      "cpu": 512,
      "memory": 1024,
      "portMappings": [
        {
          "name": "http",
          "containerPort": 19091,
          "hostPort": 19091,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "secrets": [
        {
          "name": "ACTIVE_PROFILE",
          "valueFrom": "arn:aws:ssm:ap-northeast-2:408891294463:parameter/config/loko/ACTIVE_PROFILE"
        },
        {
          "name": "EC2_MIDDLEWARE_IP",
          "valueFrom": "arn:aws:ssm:ap-northeast-2:408891294463:parameter/config/loko-feed/EC2_MIDDLEWARE_IP"
        },
        {
          "name": "EUREKA_HOST",
          "valueFrom": "arn:aws:ssm:ap-northeast-2:408891294463:parameter/config/loko/EUREKA_HOST"
        },
        {
          "name": "JWT_SECRET_KEY",
          "valueFrom": "arn:aws:ssm:ap-northeast-2:408891294463:parameter/config/loko/JWT_SECRET_KEY"
        },
        {
          "name": "REDIS_PASSWORD",
          "valueFrom": "arn:aws:ssm:ap-northeast-2:408891294463:parameter/config/loko-feed/REDIS_PASSWORD"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/loko-gateway",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::408891294463:role/loko-task-role",
  "taskRoleArn": "arn:aws:iam::408891294463:role/loko-task-role"
}